{
  "name": "@enigmatis/polaris-nest",
  "version": "1.6.1",
  "description": "Nest TypeScript example repository",
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "rebuild": "npm run clean && npm run build",
    "coverage": "jest --runInBand --detectOpenHandles --coverage",
    "prepare": "npm run lint:fix && npm run build",
    "lint": "npm run tslint-check && tslint -p .",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "lint:fix": "npm run tslint-check && tslint -p . --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "author": "enigmatis",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Enigmatis/polaris-united.git"
  },
  "keywords": [
    "polaris",
    "graphql",
    "typescript"
  ],
  "dependencies": {
    "@enigmatis/polaris-core": "^2.0.0-beta.59",
    "@nestjs/common": "^7.6.12",
    "@nestjs/core": "^7.6.12",
    "@nestjs/graphql": "^7.9.9",
    "@nestjs/platform-express": "^7.6.12",
    "@nestjs/typeorm": "^7.1.5",
    "apollo-server": "^2.17.0",
    "apollo-server-plugin-base": "0.6.10",
    "graphql": "^15.3.0",
    "graphql-subscriptions": "1.1.0",
    "pg": "^8.4.2",
    "rxjs": "6.5.4",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-angular": "8.3.4",
    "@nestjs/cli": "^7.5.5",
    "@nestjs/testing": "^7.6.12",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.6.4",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "jest": "^26.4.2",
    "lint-staged": "10.0.9",
    "prettier": "^2.1.1",
    "reflect-metadata": "^0.1.13",
    "release-it": "^13.6.1",
    "rxjs-compat": "6.5.4",
    "ts-jest": "^26.3.0",
    "ts-loader": "6.2.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "3.9.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^4.0.2"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.7.0 || ^7.0.0",
    "@nestjs/core": "^6.7.0 || ^7.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.0.0",
    "typeorm": "^0.2.7"
  }
}
