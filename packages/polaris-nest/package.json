{
  "name": "@enigmatis/polaris-nest",
  "version": "1.1.0",
  "description": "Nest TypeScript starter repository",
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "test": "jest --runInBand --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'",
    "semantic-release": "semantic-release",
    "prepare": "npm run lint && npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/npm",
        {
          "npmPublish": true
        }
      ],
      "@semantic-release/github"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      {
        "path": "@semantic-release/git",
        "assets": [
          "package.json",
          "package-lock.json",
          "CHANGELOG.md"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ]
  },
  "author": "enigmatis",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Enigmatis/polaris-nest.git"
  },
  "keywords": [
    "polaris",
    "graphql",
    "typescript"
  ],
  "dependencies": {
    "@enigmatis/polaris-core": "2.0.0-beta.18",
    "apollo-server-plugin-base": "^0.8.0",
    "@nestjs/common": "7.0.5",
    "@nestjs/core": "7.0.5",
    "@nestjs/graphql": "^7.3.9",
    "@nestjs/typeorm": "^7.0.0",
    "@nestjs/platform-express": "^7.0.11",
    "apollo-server": "2.11.0",
    "graphql": "14.6.0",
    "graphql-subscriptions": "1.1.0",
    "pg": "^7.18.2",
    "rxjs": "6.5.4",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-angular": "8.3.4",
    "@nestjs/cli": "^7.1.5",
    "@nestjs/testing": "7.0.5",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/express": "4.17.3",
    "@types/jest": "25.1.4",
    "@types/node": "10.17.3",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.1",
    "graphql-request": "^1.8.2",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "lint-staged": "10.0.9",
    "prettier": "2.0.2",
    "reflect-metadata": "0.1.13",
    "release-it": "^13.6.1",
    "rxjs-compat": "6.5.4",
    "semantic-release": "^17.0.4",
    "ts-jest": "25.2.1",
    "ts-loader": "6.2.2",
    "ts-node": "8.8.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.8.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.7.0 || ^7.0.0",
    "@nestjs/core": "^6.7.0 || ^7.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0",
    "typeorm": "^0.2.7"
  }
}
