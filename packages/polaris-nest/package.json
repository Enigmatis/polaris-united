{
  "name": "@enigmatis/polaris-nest",
  "version": "1.1.5",
  "description": "Nest TypeScript example repository",
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "coverage": "jest --runInBand --detectOpenHandles --coverage",
    "prepare": "npm run lint:fix && npm run build",
    "lint": "npm run tslint-check && tslint -p .",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "lint:fix": "npm run tslint-check && tslint -p . --fix",
    "test": "jest --runInBand --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'"
  },
  "author": "enigmatis",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Enigmatis/polaris-nest.git"
  },
  "keywords": [
    "polaris",
    "graphql",
    "typescript"
  ],
  "dependencies": {
    "@enigmatis/polaris-core": "^2.0.0-beta.40",
    "@enigmatis/polaris-logs": "^4.2.0",
    "apollo-server-core": "^2.17.0",
    "apollo-server-express": "^2.17.0",
    "apollo-server-fastify": "^2.17.0",
    "apollo-server-plugin-base": "0.6.10",
    "express": "^4.17.1",
    "@nestjs/common": "7.0.5",
    "@nestjs/core": "7.0.5",
    "@nestjs/graphql": "^7.3.9",
    "@nestjs/typeorm": "^7.0.0",
    "@nestjs/platform-express": "^7.0.11",
    "apollo-server": "^2.17.0",
    "graphql": "^15.3.0",
    "graphql-subscriptions": "1.1.0",
    "pg": "^7.18.2",
    "rxjs": "6.5.4",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-angular": "8.3.4",
    "@nestjs/cli": "^7.1.5",
    "@nestjs/testing": "7.0.5",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.13",
    "@types/node": "^14.6.4",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.1",
    "graphql-request": "3.0.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "10.0.9",
    "prettier": "^2.1.1",
    "reflect-metadata": "^0.1.13",
    "release-it": "^13.6.1",
    "rxjs-compat": "6.5.4",
    "ts-jest": "^26.3.0",
    "ts-loader": "6.2.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "^4.0.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-consistent-codestyle": "^1.16.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^2.3.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^6.7.0 || ^7.0.0",
    "@nestjs/core": "^6.7.0 || ^7.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.0.0",
    "typeorm": "^0.2.7"
  }
}
